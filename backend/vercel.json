{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
        },
        {
            "src": "/(.*)",
            "dest": "server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
        }
    ]
    ,"env": {
        "NODE_VERSION": "22.x"
    }
}
